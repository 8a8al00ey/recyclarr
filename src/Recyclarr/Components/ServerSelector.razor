@typeparam TConfig

<MudSelect
    T="TConfig"
    Value="@Selection"
    ValueChanged="OnSelectionChanged"
    Label="@Label"
    Class="mt-3 mt-sm-0">
    @foreach (var instance in ConfigRepo.Configs)
    {
        <MudSelectItem @key="@instance.Id" Value="@instance">@instance.BaseUrl</MudSelectItem>
    }
</MudSelect>
